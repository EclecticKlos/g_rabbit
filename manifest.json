{
  "manifest_version": 2,
  "name": "pledgmail",
  "description": "Helps you write and receive shorter emails",
  "version": "0.0.0.1",
  "default locale": "en",
  "icons": { "128": "imgs/pledge_pin.png"},
  "content_scripts" : [
    {
      "matches": ["*://mail.google.com/mail/*"],
      "js": ["js/jquery.js", "js/compose.js"],
      "css": ["css/stylesheet.css"]
    }
  ]
}
